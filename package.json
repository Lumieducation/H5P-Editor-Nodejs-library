{
    "name": "h5p-editor-nodejs-library",
    "version": "v0.0.0",
    "description": "The H5P-Editor-Nodejs-library is a port of the H5P-Editor-PHP-library for Nodejs.",
    "repository": {
        "type": "git",
        "url": "https://github.com/Lumieducation/H5P-Editor-Nodejs-library"
    },
    "scripts": {
        "postinstall": "scripts/download-core.sh 1.22.0 && cp -r src/client h5p/editor/wp && npm run download:content",
        "start": "node ./examples/express",
        "clean": "rm -rf build && rm -rf dist",
        "uninstall": "npm run clean && rm -rf node_modules",
        "ci": "npm run lint && npm run build && npm run test && npm run test:integration",
        "build": "npx babel src --out-dir build",
        "build:watch": "npx babel src --watch --out-dir build",
        "download:content": "node scripts/download-examples.js test/data/content-type-cache/real-content-types.json test/data/hub-content",
        "lint": "eslint ./src",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:integration": "node test/integration.js "
    },
    "author": "Jan Philip Schellenberg",
    "dependencies": {
        "@babel/core": "^7.5.5",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "adm-zip": "^0.4.13",
        "ajv": "^6.10.2",
        "axios": "^0.19.0",
        "babel-polyfill": "^6.26.0",
        "body-parser": "^1.19.0",
        "crc": "^3.8.0",
        "escape-html": "^1.0.3",
        "express": "^4.17.1",
        "fs-extra": "^8.1.0",
        "merge": "^1.2.1",
        "puppeteer": "^1.19.0",
        "qs": "^6.7.0",
        "unzipper": "^0.10.3",
        "shortid": "^2.2.14"
    },
    "devDependencies": {
        "@babel/cli": "^7.5.5",
        "@babel/register": "^7.5.5",
        "axios-mock-adapter": "^1.17.0",
        "babel-eslint": "^10.0.2",
        "babel-jest": "^24.8.0",
        "babel-preset-env": "^1.7.0",
        "eslint": "^6.1.0",
        "eslint-config-airbnb-base": "^13.2.0",
        "eslint-config-prettier": "^6.0.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jest": "^22.15.0",
        "express-fileupload": "^1.1.5",
        "jest": "^24.8.0",
        "mkdirp": "^0.5.1",
        "mockdate": "^2.0.4"
    },
    "main": "./src/index.js"
}
